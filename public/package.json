{
  "name": "quickfeed",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.0",
    "@bufbuild/protoc-gen-es": "^2.2.0",
    "@connectrpc/connect": "^2.0.0-rc.1",
    "@connectrpc/connect-web": "^2.0.0-rc.1",

    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "overmind": "^28.0.4-1738965119622",
    "overmind-react": "^29.0.6-1738965119622",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-markdown": "^9.0.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.3.0",
    "react-syntax-highlighter": "^15.6.1"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:ci": "jest --testPathIgnorePatterns=e2e --maxWorkers=1",
    "test:selenium": "jest --testPathPattern=e2e",
    "build": "webpack",
    "build:watch": "webpack --watch --mode development",
    "build:dev": "webpack --mode development"
  },
  "devDependencies": {
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.2.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/selenium-webdriver": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "css-loader": "^6.7.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "jsdom": "^20.0.1",
    "process": "0.11.10",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "selenium-webdriver": "^4.2.0",
    "source-map-loader": "^2.0.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^5.3.4",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "setupFiles": [
      "<rootDir>/src/__tests__/setup.jest.js"
    ],
    "testEnvironment": "jsdom",
    "verbose": true,
    "transformIgnorePatterns": [
      "!node_modules/(?!@src/*.)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "!**/__tests__/TestHelpers.ts",
      "!**/__tests__/mock_data/**",
      "!**/__tests__/setup.jest.js"
    ],
    "moduleNameMapper": {
      "./types_pb.js": "<rootDir>/proto/qf/types_pb.ts",
      "./requests_pb.js": "<rootDir>/proto/qf/requests_pb.ts",
      "../kit/score/score_pb.js": "<rootDir>/proto/kit/score/score_pb.ts"
    }
  }
}
